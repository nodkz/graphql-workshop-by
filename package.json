{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "react": "^16.8.5",
    "react-apollo": "^2.5.2",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-dom": "^16.8.5",
    "react-router": "^5.0.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.1",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.11",
    "@types/node": "^11.11.4",
    "@types/qs": "^6.5.2",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "apollo": "^2.6.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    "typescript": "^3.3.4000"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,ts,tsx,jsx}'",
    "apollo-generate": "apollo client:codegen --addTypename --target=typescript --localSchemaFile=./schema.graphql --includes=\"./src/**/*.{ts,tsx}\" --tagName gql",
    "watch-generate": "npm run apollo-generate -- --watch",
    "watch": "npm run clear && concurrently -k npm:watch-generate npm:start",
    "clear": "find ./src -name \"__generated__\" -exec rm -rf '{}' +"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
